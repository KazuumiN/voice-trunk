    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0",
    "wrangler": "^3.100.0"
  },
  "overrides": {
    "@sqlite.org/sqlite-wasm": "3.48.0-build2"
  },
  "dependencies": {
